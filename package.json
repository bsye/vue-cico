{
  "name": "vue-cico",
  "version": "2.1.2",
  "description": "Smartbox version of check-in check-out",
  "author": "Smartbox",
  "main": "dist/vueCico.common.js",
  "repository": {
    "url": "git@gitlab.production.smartbox.com:npm/vue-cico.git",
    "type": "git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "date",
    "dates",
    "input",
    "date input",
    "datepicker",
    "date picker",
    "range",
    "date range picker",
    "vue-datepicker",
    "vue-hotel-datepicker",
    "vue-date-range"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service test:unit && vue-cli-service build --target lib --name vueCico ./src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/*.vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "fecha": "^4.2.1",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.15.2",
    "eslint": "^7.30.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.13.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.14"
  }
}
